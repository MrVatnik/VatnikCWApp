<Window x:Class="VatnikCWApp.EditElementWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VatnikCWApp"
        mc:Ignorable="d"
        Title="EditElementWindow" Height="150" Width="804">
    <Grid Margin="0,0,-0.4,0.2" Width="794">
        <DataGrid x:Name="EEDataGrid" AutoGenerateColumns ="True" Height="49" Margin="10,10,10,0" VerticalAlignment="Top" AutoGeneratingColumn="EEDataGrid_AutoGeneratingColumn"/>
        <Button x:Name="SaveButton" Content="Save" Margin="10,0,0,10" Click="SaveButton_Click" VerticalAlignment="Bottom" Width="75" HorizontalAlignment="Left"/>
        <Button x:Name="DeleteButton" Content="Delete
" HorizontalAlignment="Left" Margin="90,0,0,10" VerticalAlignment="Bottom" Width="75" Click="DeleteButton_Click"/>

    </Grid>
</Window>
