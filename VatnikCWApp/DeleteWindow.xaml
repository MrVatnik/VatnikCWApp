<Window x:Class="VatnikCWApp.DeleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VatnikCWApp"
        mc:Ignorable="d"
        Title="DeleteWindow" Height="171.468" Width="804">
    <Grid>
        <Label x:Name="label" Content="Are you sure this Element should be deleted?" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top"/>
        <DataGrid x:Name="DeletedElementDataGrid" Margin="10,41,10,51.6" Height="49" AutoGeneratingColumn="DeletedElementDataGrid_AutoGeneratingColumn"/>
        <Button x:Name="DeleteButton" Content="Delete" Margin="10,0,0,9.6" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="75" Click="DeleteButton_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,9.6" VerticalAlignment="Bottom" Width="75" Click="CancelButton_Click"/>

    </Grid>
</Window>
