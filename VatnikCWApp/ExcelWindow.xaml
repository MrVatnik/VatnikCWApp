<Window x:Class="VatnikCWApp.ExcelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VatnikCWApp"
        mc:Ignorable="d"
        Title="ExcelWindow" Height="146" Width="404">
    <Grid>
        <TextBox x:Name="ExTextBox" Height="23" Margin="10,36,40.6,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="Enter filename" HorizontalAlignment="Center" Margin="10,10,9.6,0" VerticalAlignment="Top" Width="378"/>
        <Label x:Name="label1" Content=".xls" Margin="0,36,9.6,0" VerticalAlignment="Top" HorizontalAlignment="Right" RenderTransformOrigin="0.531,0.648"/>
        <Button x:Name="SaveButton" Content="Save" Margin="10,0,9.6,10" VerticalAlignment="Bottom" Click="SaveButton_Click"/>

    </Grid>
</Window>
